max_clients 5;

413 ./error_pages/error_page_413;
500 ./error_pages/error_page_500;
501 ./error_pages/error_page_501;

  server {
    listen          6969;
    host            127.0.0.1;
    server_name     www.domain1.com;
    root            www/html;
    max_body_size   100000000;
    methods         GET,POST,DELETE;
    index           index.html;


    location [
      autoindex on;
      fastcgi   .py|cgi-bin/;
	]
  }

  server {
    listen      6968;
    host        127.0.0.1;
    server_name www.domain2.com;
    root        www/html1;
    max_body_size   100000000;
    methods     GET,POST,DELETE;
    index       index.html;

    location [
      autoindex on;
      fastcgi   .py|cgi-bin/;
	]
  }

  server {
    listen      6970;
    host        127.0.0.1;
    server_name www.domain3.com;
    root        www/siege;
    max_body_size   100000;
    methods     GET,POST;
    index       index.html;

    location [
      autoindex off;
      fastcgi   .py|cgi-bin/;
	]
  }
